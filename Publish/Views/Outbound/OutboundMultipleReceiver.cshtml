    function FormatSessionDetails(d) {

        var Receiver = 'CustomerName: ' + d[3] + ' - ' + 'ReceiverID: ' + d[0] + ' - ' + 'BillingID: ' + d[17];
        var ClassReceiver = d[0] + '|' + d[1] + '|' + d[17];

        $("#lstReceivers").sortable({
            placeholder: "ui-state-highlight"
        });

        var result = $('#lstReceivers').sortable('toArray', { attribute: 'value' });

        if (result == "") {
            $('#lstReceivers').append("<li class= " + ClassReceiver + " value = " + d[0] + " > " + Receiver + ' ' + " <button delete class='btn btn-primary btn-xs'>Remove</button></li>");

        }
        else {
            var checkReceiver = true;
            $.each(result, function (i, val) {
                if (d[0] == result[i]) {
                    checkReceiver = false;
                }
            });

            if (checkReceiver == true) {
                $('#lstReceivers').append("<li class= " + ClassReceiver + " value = " + d[0] + " > " + Receiver + ' ' + " <button delete class='btn btn-primary btn-xs'>Remove</button></li>");
            }
            else {
                swal("Receiver Already Added.")
            }
        }

